{
    "name": "puins/think-addons",
    "description": "thinkphp6 插件包",
    "authors": [
        {
            "name": "puins",
            "email": "hnjumeng@163.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0.0",
        "topthink/think-view": "^1.0",
        "topthink/think-helper": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\service\\AddonsService"
            ],
            "config":{
                "addons": "src/config.php"
            }            
        }
    }
}
